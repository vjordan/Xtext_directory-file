<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="dirFile" nsURI="http://www.xtext.org/example/DirFile" nsPrefix="dirFile">
  <eClassifiers xsi:type="ecore:EClass" name="Directory" eSuperTypes="#//TYPE_DIR_FILE">
    <eStructuralFeatures xsi:type="ecore:EReference" name="tree" upperBound="-1" eType="#//TYPE_DIR_FILE"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="File" eSuperTypes="#//TYPE_DIR_FILE"/>
  <eClassifiers xsi:type="ecore:EClass" name="TYPE_DIR_FILE">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FirstDirectory">
    <eStructuralFeatures xsi:type="ecore:EReference" name="firstDirectory" eType="#//Directory"
        containment="true"/>
  </eClassifiers>
</ecore:EPackage>
